			MX6UL_PAD_UART3_RX_DATA__UART3_DCE_RX 0x1b0b1
			/*MX6UL_PAD_UART3_CTS_B__UART3_DCE_CTS 0x1b0b1 */
			MX6UL_PAD_UART3_CTS_B__GPIO1_IO26 0x170f9 
		>;
	};

	pinctrl_uart4: uart4grp {
		fsl,pins = <
			MX6UL_PAD_UART4_TX_DATA__UART4_DCE_TX 0x1b0b1
