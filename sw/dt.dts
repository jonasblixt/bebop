		fsl,pins = <
			MX6UL_PAD_LCD_DATA11__GPIO3_IO16	0x17059 /* 5V_USB_EN */
			MX6UL_PAD_GPIO1_IO03__GPIO1_IO03	0x17059 /* IMX_5V_USB_EN */
